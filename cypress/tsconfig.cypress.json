{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": ["esnext", "dom"],
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["cypress"]
  },
  "include": ["cypress/**/*.ts", "cypress/**/*.tsx"]
}
